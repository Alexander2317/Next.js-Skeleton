{
  "name": "boilerplate-next-js",
  "version": "3.0.2",
  "description": "Next.js boilerplate",
  "license": "ISC",
  "keywords": [
    "next js sekelton",
    "next js boilerplate",
    "boilerplate next js",
    "next js boilerplate with redux saga",
    "next js boilerplate with redux",
    "next js boilerplate with immutable"
  ],
  "author": "Alexander2317",
  "bugs": {
    "url": "https://github.com/Alexander2317/boilerplate-next-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexander2317/boilerplate-next-js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "yarn fix",
      "yarn test"
    ]
  },
  "dependencies": {
    "@zeit/next-source-maps": "^0.0.3",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "immutable": "^4.0.0-rc.12",
    "next": "^10.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^7.0.1",
    "next-images": "^1.6.2",
    "next-redux-saga": "^5.0.0-0",
    "next-redux-wrapper": "^6.0.2",
    "next-routes": "^1.4.2",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-transform-flow-strip-types": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "flow-bin": "^0.141.0",
    "flow-typed": "^3.2.1",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.3",
    "pleeease-filters": "^4.0.0",
    "postcss": "^8.2.1",
    "postcss-calc": "^7.0.5",
    "postcss-color-function": "^4.1.0",
    "postcss-css-variables": "^0.17.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^10.0.0",
    "postcss-custom-selectors": "^5.1.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-mixins": "^7.0.2",
    "postcss-nested": "^5.0.3",
    "postcss-nesting": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-selector-matches": "^4.0.0",
    "postcss-selector-not": "^4.0.1",
    "postcss-simple-vars": "^6.0.2",
    "prettier": "^2.2.1",
    "prettier-cli": "^0.1.0",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "webpack": "^5.11.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint:js": "eslint --ext .js --ext .jsx --ignore-pattern \"!**/.*\" .",
    "lint:css": "stylelint \"**/*.{css,pcss}\"",
    "lint": "yarn lint:js && yarn lint:css",
    "fix:js": "yarn lint:js --fix && yarn prettier-eslint --write",
    "fix:css": "yarn lint:css --fix",
    "fix": "yarn fix:js && yarn fix:css",
    "test": "jest --no-cache --passWithNoTests",
    "test:coverage": "jest --no-cache --coverage --passWithNoTests",
    "flow": "flow",
    "build": "next build",
    "build:analyze": "cross-env BUNDLE_ANALYZE=both yarn build",
    "build:docker": "docker build - < Dockerfile",
    "dev": "babel-node server",
    "start": "cross-env NODE_ENV=production babel-node server"
  }
}
