  "scripts": {
    "precommit": "lint-staged",
    "lint-js": "eslint --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"**/*.{css,pcss}\"",
    "lint": "yarn lint-js && yarn lint-css",
    "fix-js": "yarn lint-js --fix && yarn prettier-eslint --write",
    "fix-css": "yarn lint-css --fix",
    "fix": "yarn fix-js && yarn fix-css",
    "test": "jest --no-cache --passWithNoTests",
    "flow": "flow",
    "build": "next build",
    "build:analyze": "cross-env BUNDLE_ANALYZE=both yarn build",
    "buildDocker": "docker build - < Dockerfile",
    "dev": "babel-node server",
    "start": "cross-env NODE_ENV=production babel-node server"
  }
