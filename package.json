    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "immutable": "^4.0.0-rc.12",
    "next": "^9.5.3",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.6.0",
    "next-redux-saga": "^5.0.0-0",
